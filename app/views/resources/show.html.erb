<div class="res-show-container">
  <h1 class="res-name-head"><%= link_to "#{@resouce.name}", "#{@resouce.resource_link}", target: :_blank%></h1>
  <p><i class="fa fa-thumbs-up" aria-hidden="true"></i> : <%= @resouce.get_likes.size %> <i class="fa fa-thumbs-down" aria-hidden="true"></i> : <%= @resouce.get_dislikes.size %></p>
  <br>
  <div class="addthis_inline_share_toolbox"></div>
  <p class="res-name-para"><%= @resouce.description%></p>
  <% if @resouce.resource_link.include? "youtube" %>
    <% str1 = "#{@resouce.resource_link}" %>
    <% str2 = str1.split("=") %>
    <%= content_tag(:iframe,'', width: 840, height: 630, src: "https://www.youtube.com/embed/#{str2[1]}") %>
    <% end %>
</div>

