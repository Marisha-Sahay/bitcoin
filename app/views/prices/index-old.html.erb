									</p>
									<% else %>
									<div class="checkbox">
										<input type="checkbox" id="toggle-vis-<%= column[1] %>" value="<%= column[1] %>" class="toggle-vis" <% if column[2] == true %> <%= "checked" %> <% end %> >
										<label for="toggle-vis-<%= column[1] %>"><%= column[0] %></label>
									</div>
									<% end %>
								<% end %>
